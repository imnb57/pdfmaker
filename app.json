{
  "expo": {
    "name": "pdfmaker",
    "slug": "pdfmaker",
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/7cbab8d7-4322-4c3b-9b34-e3ea22d8f4e8"
    },
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ],
      "package": "com.codernitesh.pdfmaker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7cbab8d7-4322-4c3b-9b34-e3ea22d8f4e8"
      }
    },
    "runtimeVersion": "1.0.0",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow this app to access your photos to choose images.",
          "cameraPermission": "Allow this app to access your camera to take photos."
        }
      ]
    ]
  }
}
